<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.briup.dao.ICustomDao">
<select id="findCusByName" parameterType="String" resultType="Customer">
    select *
    from es_customer
    where name =#{name}
</select>
    <insert id="AddCustomer" parameterType="Customer" useGeneratedKeys="true" keyProperty="id">
        insert into
        es_customer(name,password,zipCode,address,telephone,email)
        values(#{name},#{password},#{zipCode},#{address},#{telephone},#{email})
    </insert>
</mapper>